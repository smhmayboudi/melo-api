    "test:cov": "jest --config ./jest.config.json --coverage",
    "test:debug": "node --inspect-brk=0.0.0.0:9229 --require tsconfig-paths/register --require ts-node/register node_modules/.bin/jest --config ./jest.config.json --detect-open-handles --run-in-band",
