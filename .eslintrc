{
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/recommended-requiring-type-checking",
    "plugin:prettier/recommended",
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/standard",
    "plugin:jest/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "globalReturn": true,
      "impliedStrict": true,
      "jsx": false
    },
    "ecmaVersion": 6,
    "project": "./tsconfig.eslint.json",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "root": true,
  "rules": {
    "@typescript-eslint/camelcase": ["error", { "allow": [
      "auth_date",
      "callback_query",
      "created_at",
      "download_count",
      "downloads_count",
      "e_tag",
      "expire_at",
      "file_key",
      "first_name",
      "followers_count",
      "is_blocked",
      "is_bot",
      "language_code",
      "last_name",
      "mime_type",
      "owner_user_id",
      "photo_id",
      "photo_url",
      "private_key",
      "public_key",
      "registered_date",
      "release_date",
      "songs_ids",
      "telegram_id",
      "tracks_count",
      "update_id",
      "user_id"
    ]}],
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "sort-imports":"error",
    "sort-keys":"error",
    "sort-vars":"error"
  }
}
